{
  "name": "helloWorld",
  "description": "",
  "version": "1.0.0",
  "oc": {
    "files": {
      "data": "server.js",
      "template": {
        "src": "app.js",
        "type": "oc-template-react"
      }
    },
    "parameters": {
      "name": {
        "default": "World",
        "description": "Your name",
        "example": "Jane Doe",
        "mandatory": false,
        "type": "string"
      },
      "shadow": {
        "default": false,
        "description": "Do you want to enable shadow DOM",
        "mandatory": false,
        "type": "boolean"
      }
    }
  },
  "devDependencies": {
    "oc-template-react-compiler": "5.2.2"
  },
  "dependencies": {
    "npm": "^8.1.2",
    "react-shadow-root": "^6.1.0"
  }
}
